{{ form_start(filter_cars) }}
<div class="d-flex justify-content-between gap-4 text-white">

    <div class="w-100">
        {{ form_label(filter_cars.brand, 'Марка:') }}

        {{ form_widget(filter_cars.brand, {
            placeholder: 'Выберите марку ...',
            attr: { 'data-select' : 'select2', class: 'rounded-4 py-2 w-100 mb-0' } }) }}
    </div>

    <div class="w-100">
        {{ form_label(filter_cars.model, 'Модель:') }}

        {{ form_widget(filter_cars.model, {
            label: 'Модель:',
            placeholder: 'Выберите модель ...',
            attr: { 'data-select' : 'select2', class: 'rounded-4 py-2 w-100 mb-0' } }) }}
    </div>

    <div class="w-100">
        {{ form_label(filter_cars.modification, 'Модификация:') }}

        {{ form_widget(filter_cars.modification, {
            label: 'Модификация:',
            placeholder: 'Выберите модификацию ...',
            attr: { 'data-select' : 'select2', class: 'rounded-4 py-2 w-100 mb-0' } }) }}
    </div>

    {# ПОДОБРАТЬ ШИНЫ #}
    <div class="w-100">
        {# <button class="btn btn-danger w-100 h-100 rounded-4 text-uppercase fw-bolder"
												value="подобрать шины"
												aria-label="Подобрать шины"
										>подобрать шины</button> #}

        {{ form_widget(filter_cars.cars_filter,
            {
                label: 'Подобрать шины',
                attr: { class: 'btn-danger w-100 h-100 rounded-4 text-uppercase fw-bolder'
            }}
        ) }}
    </div>
</div>

<div class="row justify-content-between mb-3 mt-4">

    <div class="radio-buttons d-flex gap-2 w-75">


        {% for season in filter_cars.season %}

            {# <div class="pe-2"> #}
            <label for="{{ season.vars.id }}">

                <input
                        id="{{ season.vars.id }}"
                        name="{{ field_name(season) }}"
                        value="{{ field_value(season) }}"
                        type="radio"
                        {{ field_value(season) is empty ? 'checked="checked"' }}
                        class="form-check-input visually-hidden"
                        aria-label="{{ field_value(season) }}"
                >

                {% if field_value(season) is empty %}
                    <span class="icon icon-sm rounded-3 small px-5 text-primary">
                            Все
                        </span>
                {% endif %}


                {% if field_value(season) == 'summer' %}
                    <span class="icon icon-sm rounded-3"
                          role="tooltip"
                          data-bs-toggle="tooltip"
                          data-bs-placement="bottom"
                          data-bs-delay='{"show":600,"hide":100}'
                          title="{{ 'label'|trans }} {{ 'summer'|trans }}"
                    >
                            {# <img data-src="data:image/svg+xml;base64,"
                                 width="20" height="20"
                                 src="/assets/img/empty.png"
                                 class="lazy"
                                 alt="Летняя"
                            > #}

                            <span data-bg="data:image/svg+xml;base64,"
                                  class="lazy bg-contain"
                                  style="width: 20px; height: 20px;">
                                &nbsp;
                            </span>

						</span>
                {% endif %}

                {% if field_value(season) == 'winter' %}
                    <span class="icon icon-sm rounded-3"
                          role="tooltip"
                          data-bs-toggle="tooltip"
                          data-bs-placement="bottom"
                          data-bs-delay='{"show":600,"hide":100}'
                          title="{{ 'label'|trans }} {{ 'winter'|trans }}"
                    >
                            {# <img data-src="data:image/svg+xml;base64,"
                                 width="20" height="20"
                                 src="/assets/img/empty.png"
                                 class="lazy"
                                 alt="Зимняя"
                            > #}
                        <span data-bg="data:image/svg+xml;base64,"
                              class="lazy bg-contain"
                              style="width: 20px; height: 20px;">
                            &nbsp;
                        </span>
					</span>
                {% endif %}

                {% if field_value(season) == 'all' %}
                    <span class="icon icon-sm rounded-3"
                          role="tooltip"
                          data-bs-toggle="tooltip"
                          data-bs-placement="bottom"
                          data-bs-delay='{"show":600,"hide":100}'

                          title="{{ 'label'|trans }} {{ 'all'|trans }}}"
                    >
                            {# <img data-src="data:image/svg+xml;base64,"
                                 width="20" height="20"
                                 src="/assets/img/empty.png"
                                 class="lazy"
                                 alt="Всесезонная"
                            > #}

                        <span data-bg="data:image/svg+xml;base64,"
                              class="lazy bg-contain"
                              style="width: 20px; height: 20px;">
                            &nbsp;
                        </span>
                    </span>
                {% endif %}


            </label>
            {# </div> #}

        {% endfor %}


        <div class="pe-2">
            <label for="{{ filter_cars.studs.vars.id }}">

                <input
                        id="{{ filter_cars.studs.vars.id }}"
                        name="{{ field_name(filter_cars.studs) }}"
                        value="{{ field_value(filter_cars.studs) }}"
                        type="checkbox"

                        class="form-check-input visually-hidden"
                        aria-label="{{ field_value(filter_cars.studs) }}"
                >

                <span class="icon icon-sm rounded-3"
                      role="tooltip"
                      data-bs-toggle="tooltip"
                      data-bs-placement="bottom"
                      data-bs-delay='{"show":600,"hide":100}'
                      title="{{ 'label'|trans }}"
                >
                            {# <img data-src="data:image/svg+xml;base64,"
                                 width="20" height="20"
                                 src="/assets/img/empty.png"
                                 class="lazy"
                                 alt="Шипы"
                            > #}

                    <span data-bg="data:image/svg+xml;base64,"
                          class="lazy bg-contain"
                          style="width: 20px; height: 20px;">
                    </span>
				</span>

            </label>
        </div>


    </div>

    <div class="col d-flex justify-content-end align-items-center">
        <div class="small text-white">
            <span class="opacity-50">Свыше</span>
            <a href="/catalog/triangle" class="text-white px-1">3000 моделей</a>
            <span class="opacity-50">в каталоге</span>

        </div>
    </div>
</div>

{{ form_end(filter_cars) }}


