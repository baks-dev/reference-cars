<?xml version="1.0"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for model in models %}
        <url>
            <loc>{{ absolute_url(path('reference-cars:user.model', { brand: brand.url, model: model.url } )) }}</loc>
            <lastmod>{{ model.last_modify|date('Y-m-d\\TH:i:sP') }}</lastmod>
        </url>
    {% endfor %}
</urlset>
